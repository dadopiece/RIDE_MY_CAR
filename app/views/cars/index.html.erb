<% if user_signed_in? %>
  <%= button_to "Se déconnecter", destroy_user_session_path, method: :delete %>
<% else %>
  <%= link_to "Créer un compte", new_user_registration_path %>
  <%= link_to "Se connecter", new_user_session_path %>
<% end %>
Index
<% @cars.each do |car| %>
  <div>
    <% if car.photo.attached? %>
      <%= cl_image_tag car.photo.key %>
    <% end %>
    Model: <%= car.model %>
    Brand: <%= car.brand %>
    Price: <%= car.price %>
  </div>
<% end %>

 <% @cars.each do |car| %>
    <div class="card">
      <h2><%= car.model %></h2>
      <h2><%= car.brand %></h2>
      <h2><%= car.price %></h2>
      <!-- Vous pouvez ajouter plus d'informations ou de balises ici selon vos besoins -->
    </div>
  <% end %>
